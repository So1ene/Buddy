<%= link_to root_path, class: "back-button" do %>
  <i class="fas fa-arrow-left"></i>
<% end %>

<div class="container justify-content-start mt-0">
  <h1 class="pb-3 mt-5 pt-0">Inbox</h1>
  <% @my_requests.each do |request| %>
    <%= render partial: "inbox_message", locals: { request: request } %>
  <% end %>

  <% @incoming_requests.each do |request| %>
    <%= render partial: "inbox_message_incoming", locals: { request: request } %>
  <% end %>




</div>


